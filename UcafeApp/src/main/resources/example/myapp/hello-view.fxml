<?xml version="1.0" encoding="UTF-8"?>

<?import digit.Tablo?>
<?import javafx.scene.control.Button?>
<?import digit.Logo?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<AnchorPane id="pane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.2" fx:controller="example.myapp.HelloController">
    <StackPane AnchorPane.topAnchor="40.0" AnchorPane.rightAnchor="180">
        <Logo fx:id="logo" scaleX="0.9" scaleY="0.9"/>
    </StackPane>
    <Button fx:id="button" onAction="#onHelloButtonClick" text="Let's GO :)"/>
    <Tablo fx:id="tablo" layoutX="550" layoutY="500" scaleX="1.6" scaleY="1.6"/>
</AnchorPane>
